---
// Component Imports
import Counter from '../components/Counter.svelte';
import Header from '../components/generic/Header2.astro';
import BluredContainer from '../components/generic/BluredContainer.astro';
import SectionHeader from '../components/generic/SectionHeader.astro';

// Layout Imports
import BaseLayout from '../layouts/BaseLayout.astro';

// Full Astro Component Syntax:
// https://docs.astro.build/core-concepts/astro-components/
---
<BaseLayout title="Edgebox">
	<div slot="content">
		<Header>
			<div slot="content">
				<div class="row">
					<div class="col-lg-7 text-center mx-auto mt-8">
						<h1 class="text-white pt-3 mt-n5">Own Your Data</h1>
						<p class="lead text-white mt-3">The integrated web services host and automated data backups solution. <br /> Stability, Security and Privacy in a single package. </p>
					</div>
					<div class="row">
						<div class="col-4">
							<img style="padding-left: 100px;" height="300px" src="/img/box-stacking.png">
						</div>
						<div class="col-8">
							<img height="300px" src="/img/dashboard-screenshot.png">
						</div>
					</div>
				</div>
			</div>
		</Header>
		
		<section class="pt-3 pb-4" id="count-stats">
			<BluredContainer>
				<div class="row">
					<div class="col-md-4 position-relative">
						<div class="p-3 text-center">
							<h1 class="text-gradient text-primary">Private</h1>
							<h5 class="mt-3">Apps</h5>
							<p class="text-sm">Edgebox gets you covered by providing solid alternatives to paid cloud services</p>
						</div>
						<hr class="vertical dark">
					</div>
					<div class="col-md-4 position-relative">
						<div class="p-3 text-center">
							<h1 class="text-gradient text-primary">Easy</h1>
							<h5 class="mt-3">Setup</h5>
							<p class="text-sm">Get up and running, fully automated in your computer or server in minutes.</p>
						</div>
						<hr class="vertical dark">
					</div>
					<div class="col-md-4">
						<div class="p-3 text-center">
							<h1 class="text-gradient text-primary" id="state3">Free</h1>
							<h5 class="mt-3">Forever</h5>
							<p class="text-sm">Open-Source, you can take both your data and code and run it anywhere</p>
						</div>
					</div>
				</div>
			</BluredContainer>
		</section>
		
		<section class="my-5 py-5">
			<SectionHeader>
				<div slot="title">
					A strong feature set
				</div>
				<div slot="subtitle">
					Infinite possibilities
				</div>
				<div slot="text">
					Combine the power of multiple apps to automate tasks and manage your data, 
					making sure it is safe with encrypted backups, 
					and always securely accessible anywhere you are.
				</div>
			</SectionHeader>
			<div class="container mt-sm-5 mt-3">
				<div class="row">
					<div class="col-lg-3">
						<div class="position-sticky pb-lg-5 pb-3 mt-lg-0 mt-5 ps-2" style="top: 140px">
							<h3>EdgeApps</h3>
							<h6 class="text-secondary font-weight-normal pe-3">A selection of free (as in beer) apps that allow you to automate, manage and visualize your data.</h6>
							<h6 class="text-secondary font-weight-normal pe-3">Each app brings with it its own features and capabilities, so every time you install a new app, you are expanding the power of Edgebox to ccomplish new things.</h6>
						</div>
					</div>
					<div class="col-lg-9">
						<div class="row">
							<div class="col-md-4 mt-md-0">
								<a target="_blank" href="https://nextcloud.com/?utm-source=edgebox&utm-campaign=homepage">
									<div class="card shadow-lg move-on-hover min-height-160 min-height-160">
										<img class="w-100 my-auto" src="/img/section-edgeapps-nextcloud.jpeg" alt="">
									</div>
									<div class="mt-2 ms-2">
										<h6 class="mb-0">Content Collaboration</h6>
										<p class="text-secondary text-sm">via Nextcloud</p>
									</div>
								</a>
							</div>
							<div class="col-md-4 mt-md-0 mt-4">
								<a target="_blank" href="https://planka.app/?utm-source=edgebox&utm-campaign=homepage">
									<div class="card shadow-lg move-on-hover min-height-160 min-height-160">
										<img class="w-100 my-auto" src="/img/section-edgeapps-planka.jpeg" alt="planka is a kanban board application">
									</div>
									<div class="mt-2 ms-2">
										<h6 class="mb-0">Project Planning</h6>
										<p class="text-secondary text-sm">via Planka</p>
									</div>
								</a>
							</div>
							<div class="col-md-4 mt-md-0 mt-4">
								<a href="javascript:;">
									<div class="card shadow-lg move-on-hover min-height-160 min-height-160">
										<img class="w-100 my-auto" src="/img/section-edgeapps-bitwarden.jpeg" alt="bitwarden is a password manager solution">
									</div>
									<div class="mt-2 ms-2">
										<h6 class="mb-0">Password Management</h6>
										<p class="text-secondary text-sm">via Bitwarden</p>
									</div>
								</a>
							</div>
						</div>
						<div class="row mt-3">
							<div class="col-md-4 mt-md-0 mt-3">
								<a target="_blank" href="https://github.com/janeczku/calibre-web#calibre-web">
									<div class="card shadow-lg move-on-hover min-height-160 min-height-160">
										<img class="w-100 my-auto" src="/img/section-edgeapps-calibre.jpeg">
									</div>
									<div class="mt-2 ms-2">
										<h6 class="mb-0">E-Book Reading & Management</h6>
										<p class="text-secondary text-sm">via Calibre</p>
									</div>
								</a>
							</div>
							<div class="col-md-4 mt-md-0 mt-4">
								<a target="_blank" href="https://jellyfin.org/">
									<div class="card shadow-lg move-on-hover min-height-160 min-height-160">
										<img class="w-100 my-auto" src="/img/section-edgeapps-jellyfin.jpeg" alt="Jellyfin is an application that allows you to save, organize and stream your media like music and movies">
									</div>
									<div class="mt-2 ms-2">
										<h6 class="mb-0">Media Management & Streaming</h6>
										<p class="text-secondary text-sm">via Jellyfin</p>
									</div>
								</a>
							</div>
							<div class="col-md-4 mt-md-0 mt-4">
								<a target="_blank" href="https://actualbudget.com/">
									<div class="card shadow-lg move-on-hover min-height-160 min-height-160">
										<img class="w-100 my-auto" src="img/section-edgeapps-actual.jpeg" data-bs-toggle="tooltip" data-bs-placement="top" title="Pro Element" alt="testimonials">
									</div>
									<div class="mt-2 ms-2">
										<h6 class="mb-0">Finance Management</h6>
										<p class="text-secondary text-sm">via Actual</p>
									</div>
								</a>
							</div>
						</div>
